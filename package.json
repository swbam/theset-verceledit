  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "NODE_ENV=production vite preview --port 8080",
    "db:migration:apply": "node ./scripts/apply-migrations.js",
    "reset:cache": "node ./scripts/clear-cache.js",
    "db:repair": "for migration in 20250418165622 20250418165747 20250418165802 20250418165826 20250418165834 20250418165945 20250418165952 20250418170021 20250418173220; do supabase migration repair --status reverted $migration; done",
    "db:pull": "supabase db pull",
    "db:push": "supabase db push --include-all",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --project-id kzjnkqeosrycfpxjwhil --schema public > src/integrations/supabase/types.ts",
    "db:secrets": "supabase secrets set --env-file ./supabase/.env",
    "db:sync": "npm run db:repair && npm run db:pull && npm run db:push && npm run db:types && npm run db:secrets",
    "updateall": "git pull && npm run db:sync && npm run deploy:functions && npm run build && npm run verify && git add . && git commit -m \"Automated update\" && git push",
    "deploy:functions": "for func in fetch-past-setlists import-artist sync-artist sync-setlist sync-show sync-song sync-venue; do supabase functions deploy $func --no-verify-jwt; done",
    "verify": "curl https://theset-verceledit.vercel.app/api/test-db-connection",
    "updateschema": "bash scripts/update-schema.sh"
  },
